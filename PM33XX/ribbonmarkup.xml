                        </SplitButton>

                        <Button CommandName="IDM_PRINTPREVIEW" />
                        <Button CommandName="IDM_PRINT" />
                        <Button CommandName="IDM_RIBBONEXIT" />
                    </MenuGroup>
                </ApplicationMenu>
            </Ribbon.ApplicationMenu>

