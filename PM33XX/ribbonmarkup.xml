    <Application.Views >
        <Ribbon Name ="MyRibbon">

            <!-- QAT (Quick Access Toolbar) -->
            <Ribbon.QuickAccessToolbar>
                <QuickAccessToolbar >
                    <QuickAccessToolbar.ApplicationDefaults>
                        <!-- Button CommandName="Undo" ApplicationDefaults.IsChecked="true"/ -->
                        <!-- ToggleButton CommandName="ViewSource" ApplicationDefaults.IsChecked="false"/ -->
                        <Button CommandName="IDM_CONNECT" />
                        <Button CommandName="IDM_HARDCOPY" />
                    </QuickAccessToolbar.ApplicationDefaults>
                </QuickAccessToolbar>
            </Ribbon.QuickAccessToolbar>

            <Ribbon.ApplicationMenu>
                <ApplicationMenu CommandName="IDM_FILEMENU">
                    <ApplicationMenu.RecentItems>
                        <RecentItems CommandName="IDM_MRU" MaxCount="9" />
                    </ApplicationMenu.RecentItems>
                    <MenuGroup Class="MajorItems">
                        <Button CommandName="IDM_NEW" />
                        <Button CommandName="IDM_OPEN" />
                        <Button CommandName="IDM_SAVE" />

                        <SplitButton CommandName="SaveAsSplitButton" >
                            <SplitButton.ButtonItem>
