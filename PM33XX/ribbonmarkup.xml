                            </SplitButton.ButtonItem>
                            <SplitButton.MenuGroups>
                                <MenuGroup Class="MajorItems">
                                    <Button CommandName="IDM_SAVEASANS" />
                                    <Button CommandName="IDM_SAVEASXLS" />
                                    <Button CommandName="IDM_SAVEASPNG" />
                                    <Button CommandName="IDM_SAVEASBMP" />
                                </MenuGroup>
